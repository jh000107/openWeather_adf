{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DS598JunhuiCho"
		},
		"to_blob_sink_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'to_blob_sink'"
		},
		"airpollution_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org/"
		},
		"historical_weather_http_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DS598JunhuiCho_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"to_blob_sink_connectionString": {
						"value": "[parameters('to_blob_sink_connectionString')]"
					},
					"airpollution_http_properties_typeProperties_url": {
						"value": "[parameters('airpollution_http_properties_typeProperties_url')]"
					},
					"historical_weather_http_properties_typeProperties_url": {
						"value": "[parameters('historical_weather_http_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}